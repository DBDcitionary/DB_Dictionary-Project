@model IEnumerable<WebApplication1.Table_Tbl>

@{
    ViewBag.Title = "TableInformation";
}
<div class="jumbotron">
    <h1>Database Dictionary</h1>
</div>

<h2>Table Information</h2><br />

<div>
    <span class="glyphicon glyphicon-arrow-left">
        @Html.ActionLink("Back to previous page", null, null, null, new { href = Request.UrlReferrer })
    </span>
</div><br />

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<table class="table table-striped">
    <tr>
        <th> Name
           <!--@Html.DisplayNameFor(model => model.TBL_Name)-->
        </th>
        <th> Description
            <!--@Html.DisplayNameFor(model => model.TBL_Description)-->
        </th>
        <th></th>
    </tr>
@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TBL_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TBL_Description)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { TBL_ID = item.TBL_ID }) |
            @Html.ActionLink("Details", "FieldInformation", new { TBL_ID = item.TBL_ID }) |
            @Html.ActionLink("Delete", "Delete", new { TBL_ID = item.TBL_ID })
        </td>
    </tr>
}
</table>
